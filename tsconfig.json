{
    "compilerOptions": {
        "target": "es2019",
        "module": "commonjs",
        "allowJs": true,
        "checkJs": false,
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/'fastpriorityqueue"
        ],
        "declaration": true,
        "outDir": "./dist",
        "strict": true,
        "noImplicitAny": false,
        "noImplicitThis": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "**/__tests__/*",
        "examples/**/*"
    ]
}