{
    "name": "@throw-out-error/throw-out-utils",
    "version": "1.1.0",
    "main": "dist/index.js",
    "scripts": {
        "prepublishOnly": "pnpm test && pnpm lint",
        "prepare": "pnpm build",
        "version": "git add -A src",
        "test": "mocha --require ts-node/register --watch-extensions ts 'test/**/*.ts'",
        "build": "tsc --build && pnpm bundle",
        "bundle": "browserify --standalone throw-out-utils src/index.ts -p [ tsify ] > dist/bundle.js",
        "preversion": "pnpm lint",
        "lint": "eslint src/**/*.ts --quiet --fix",
        "coverage": "nyc mocha --require ts-node/register --watch-extensions ts 'test/**/*.ts'"
    },
    "files": [
        "dist/**/*"
    ],
    "keywords": [],
    "author": "throw-out-error",
    "license": "MIT",
    "directories": {
        "examples": "examples"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/throw-out-error/throw-out-utils"
    },
    "description": "",
    "dependencies": {
        "@types/assert": "^1.4.7",
        "@types/node": "^14.0.11",
        "@types/treeify": "^1.0.0",
        "assert": "^2.0.0",
        "treeify": "^1.1.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/mocha": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^3.1.0",
        "@typescript-eslint/parser": "^3.1.0",
        "browserify": "^16.5.1",
        "chai": "^4.2.0",
        "eslint": "^7.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "mocha": "^7.2.0",
        "nyc": "^15.1.0",
        "prettier": "^2.0.5",
        "ts-node": "^8.10.2",
        "tsify": "^4.0.2",
        "typescript": "^3.9.5"
    }
}
